<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="wanderingdevelopment.tk.sdkbaseui.Fragments.ContactListFragment">



    <android.support.v7.widget.RecyclerView
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/list"
        android:name="wanderingdevelopment.tk.sdkbaseui.FragmentTemplates.ThreadListFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="false"
        android:focusableInTouchMode="false"
        app:layoutManager="LinearLayoutManager"
        tools:listitem="@layout/fragment_user_list_item"
        >


    </android.support.v7.widget.RecyclerView>

    <include layout="@layout/super_card_toast" />

</FrameLayout>